{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${mod_version}",
	"name": "${mod_name}",
	"description": "${mod_description}",
	"authors": [
		"${mod_authors}"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/slayers-beasts",
		"sources": "https://github.com/InvictusSlayer/Slayers-Beasts"
	},
	"license": "${mod_license}",
	"icon": "${mod_id}.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.invictusslayer.slayersbeasts.fabric.SBFabric"
		],
		"client": [
			"net.invictusslayer.slayersbeasts.fabric.client.SBClient"
		],
		"fabric-datagen": [
			"net.invictusslayer.slayersbeasts.fabric.datagen.SBFabricDataGenerator"
		]
	},
	"mixins": [
		"${mod_id}-fabric.mixins.json"
	],
	"depends": {
		"fabricloader": ">=${fabric_version}",
		"minecraft": "~${minecraft_version}",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}